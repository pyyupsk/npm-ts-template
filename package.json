{
  "name": "npm-template",
  "version": "0.0.0",
  "description": "A TypeScript library template with modern tooling",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "knip": "knip",
    "prepare": "lefthook install",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pyyupsk/npm-template.git"
  },
  "bugs": {
    "url": "https://github.com/pyyupsk/npm-template/issues"
  },
  "homepage": "https://github.com/pyyupsk/npm-template#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@vitest/coverage-v8": "^4.0.16",
    "knip": "^5.79.0",
    "lefthook": "^2.0.13",
    "tsdown": "^0.19.0-beta.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
